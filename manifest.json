{
	"name": "JRpan Extension",
	"description": "JRpan based extension for japanese text tokenization.",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"https://www.google.com/*",
		"http://www.google.com/*"
	],
	"content_scripts": [
		{
			"matches": ["https://*/*"],
			"js": ["jquery.js", "kuromoji.js", "jrpan.js"]
		}
	]
}
